pipeline {
    agent any 
    stages {
        stage("build"){
            steps {
                script {
            sh """
            sudo aws ecr get-login-password --region us-east-1 | docker login --username AWS --password-stdin 181678829993.dkr.ecr.us-east-1.amazonaws.com
            sudo docker build -t 181678829993.dkr.ecr.us-east-1.amazonaws.com/frontend:latest .
            sudo docker push 181678829993.dkr.ecr.us-east-1.amazonaws.com/frontend:latest 
            """
        }
         }
    }
}
}
